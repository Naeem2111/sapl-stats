{
	"name": "proclubs-stats-hub-backend",
	"version": "1.0.0",
	"description": "Backend API for FC 26 Pro Clubs league statistics",
	"main": "src/server.js",
	"scripts": {
		"start": "node src/server.js",
		"dev": "nodemon src/server.js",
		"test": "jest",
		"test:watch": "jest --watch",
		"db:migrate": "prisma migrate dev",
		"db:migrate:deploy": "prisma migrate deploy",
		"db:generate": "prisma generate",
		"db:seed": "node src/database/seed.js",
		"db:import-users": "node src/database/importTestUsers.js",
		"db:export-credentials": "node src/database/exportTestCredentials.js",
		"db:set-test-passwords": "node src/database/setTestPasswords.js",
		"db:create-admins": "node src/database/createAdminAccounts.js",
		"db:migrate-player-data": "node src/database/migratePlayerData.js",
		"test:admin-login": "node test-admin-login.js",
		"db:seed-full": "npm run db:seed && npm run db:import-users",
		"db:studio": "prisma studio",
		"db:reset": "prisma migrate reset --force",
		"db:push": "prisma db push",
		"build": "npm run db:generate",
		"postinstall": "npm run db:generate"
	},
	"dependencies": {
		"@prisma/client": "^5.6.0",
		"axios": "^1.11.0",
		"bcryptjs": "^2.4.3",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"csv-parser": "^3.2.0",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-rate-limit": "^7.1.5",
		"helmet": "^7.1.0",
		"joi": "^17.11.0",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"multer": "^2.0.2",
		"prisma": "^5.6.0",
		"sharp": "^0.34.3",
		"tesseract.js": "^6.0.1",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"jest": "^29.7.0",
		"nodemon": "^3.0.2",
		"supertest": "^6.3.3"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/yourusername/proclubs-stats-hub-backend.git"
	},
	"keywords": ["proclubs", "fifa", "fc26", "stats", "api", "express", "prisma"],
	"author": "Your Name",
	"license": "MIT"
}
